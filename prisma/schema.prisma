datasource db {
  url      = env("CONFIG_DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-arm64-openssl-1.0.x"]
}

enum ConfigValueType {
  STRING
}

model Config {
  id        Int      @id @default(autoincrement())
  name      String
  type      ConfigValueType
  value     String?
}
